<c-page title="input" class=" cml-view cml-c-page">
    <view class="page-demo  cml-base cml-view">
        <c-scroller height="{{-1}}" class=" cml-view cml-c-scroller">
            <c-header title="input" class=" cml-view cml-c-header"></c-header>
            <text class="title-text  cml-base cml-text">focus聚焦 web端不支持</text>
            <view class="input-wrap  cml-base cml-view">
                <c-input cstyle="width:200cpx;" placeholder="focus聚焦 web端不支持" focus="{{isfocus}}" bindblur="_cmlEventProxy" data-eventblur="{{['bindblurevent']}}" bindfocus="_cmlEventProxy" data-eventfocus="{{['bindfocusevent']}}" bindinput="_cmlEventProxy" data-eventinput="{{['bindinputevent']}}" bindconfirm="_cmlEventProxy" data-eventconfirm="{{['bindconfirmevent']}}" class=" cml-view cml-c-input"></c-input>
            </view>
            <text class="title-text  cml-base cml-text">数据绑定,value:{{inputValue}}</text>
            <view class="input-wrap  cml-base cml-view">
                <c-input placeholder="数据绑定" value="{{inputValue}}" bindinput="_cmlEventProxy" data-eventinput="{{['inputEvent']}}" class=" cml-view cml-c-input"></c-input>
            </view>
            <text class="title-text  cml-base cml-text">监听input事件,当前事件：{{eventName}}</text>
            <view class="input-wrap  cml-base cml-view">
                <c-input placeholder="监听input事件" bindinput="_cmlEventProxy" data-eventinput="{{['testEvent']}}" bindblur="_cmlEventProxy" data-eventblur="{{['testEvent']}}" bindfocus="_cmlEventProxy" data-eventfocus="{{['testEvent']}}" bindconfirm="_cmlEventProxy" data-eventconfirm="{{['testEvent']}}" class=" cml-view cml-c-input"></c-input>
            </view>
            <text class="title-text  cml-base cml-text">最大长度是5</text>
            <view class="input-wrap  cml-base cml-view">
                <c-input placeholder="最大长度是5" maxlength="{{5}}" class=" cml-view cml-c-input"></c-input>
            </view>
            <text class="title-text  cml-base cml-text">text类型的input</text>
            <view class="input-wrap  cml-base cml-view">
                <c-input placeholder="text类型的input" type="text" class=" cml-view cml-c-input"></c-input>
            </view>

            <text class="title-text  cml-base cml-text">number类型的input</text>
            <view class="input-wrap  cml-base cml-view">
                <c-input placeholder="number类型的input" type="number" class=" cml-view cml-c-input"></c-input>
            </view>

            <text class="title-text  cml-base cml-text">number类型的input 支持最大值 最小值</text>
            <view class="input-wrap  cml-base cml-view">
                <c-input placeholder="最大值200 最小值10" value="{{testMaxMinVal}}" type="number" maxValue="{{200}}" minValue="{{10}}" bindblur="_cmlEventProxy" data-eventblur="{{['testMaxMinEvent']}}" class=" cml-view cml-c-input"></c-input>
            </view>

            <text class="title-text  cml-base cml-text">password类型的input</text>
            <view class="input-wrap  cml-base cml-view">
                <c-input placeholder="password类型的input" type="password" class=" cml-view cml-c-input"></c-input>
            </view>

            <text class="title-text  cml-base cml-text">禁用input</text>
            <view class="input-wrap  cml-base cml-view">
                <c-input placeholder="禁用input" disabled="{{true}}" class=" cml-view cml-c-input"></c-input>
            </view>

            <text class="title-text  cml-base cml-text">键盘右下键按钮search web端不支持</text>
            <view class="input-wrap  cml-base cml-view">
                <c-input placeholder="键盘右下键按钮search web端不支持" return-key-type="search" class=" cml-view cml-c-input"></c-input>
            </view>
            
            <text class="title-text  cml-base cml-text">键盘右下键按钮done web端不支持</text>
            <view class="input-wrap  cml-base cml-view">
                <c-input placeholder="键盘右下键按钮done web端不支持" return-key-type="done" class=" cml-view cml-c-input"></c-input>
            </view>

            <text class="title-text  cml-base cml-text">键盘右下键按钮next web端不支持</text>
            <view class="input-wrap  cml-base cml-view">
                <c-input placeholder="键盘右下键按钮next web端不支持" return-key-type="next" class=" cml-view cml-c-input"></c-input>
            </view>

            <text class="title-text  cml-base cml-text">键盘右下键按钮go web端不支持</text>
            <view class="input-wrap  cml-base cml-view">
                <c-input placeholder="键盘右下键按钮go web端不支持" return-key-type="go" class=" cml-view cml-c-input"></c-input>
            </view>

            <text class="title-text  cml-base cml-text">placeholderColor web端不支持</text>
            <view class="input-wrap  cml-base cml-view">
                <c-input placeholder="placeholderColor web端不支持" placer-holder-color="#ff0000;" class=" cml-view cml-c-input"></c-input>
            </view>

            <text class="title-text  cml-base cml-text">自定义样式</text>
            <view class="end-item  cml-base cml-view">
                <c-input placeholder="自定义样式" c-style="color: #00ff00; border: 1px solid #00f;" class=" cml-view cml-c-input"></c-input>
            </view>
        </c-scroller>
    </view>
</c-page>