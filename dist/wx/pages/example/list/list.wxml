<c-page title="list" class=" cml-view cml-c-page">
  <view class="list-eg  cml-base cml-view">
    <c-list bottom-offset="{{bottomOffset}}" bindscrolltobottom="_cmlEventProxy" data-eventscrolltobottom="{{['onBottom']}}" bindonscroll="_cmlEventProxy" data-eventonscroll="{{['onScroll']}}" to-element="{{toElement}}" height="{{-1}}" cstyle="width: 750cpx;" class=" cml-view cml-c-list">
        <view class=" cml-base cml-cell">
          <c-header title="list" class=" cml-view cml-c-header"></c-header>
        </view>
          <view class="cell  cml-base cml-cell  _cml_ref_lmc_" wx:for="{{lists}}" wx:for-index="i" wx:for-item="item" data-idx="{{i}}" id="cell{{i+1}}">
            <view class="panel  cml-base cml-view  _cml_ref_lmc_" id="div{{i+1}}">
              <text class="text  cml-base cml-text">{{item}}</text>
            </view>
          </view>
      </c-list>
  </view>
</c-page>