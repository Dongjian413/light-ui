<view class="cml-checkbox  cml-base cml-view cml-557bcfae">
  <view class="cml-checkbox-wrap  cml-base cml-view cml-557bcfae" onTap="_cmlEventProxy" data-eventtap="{{['changeCheck']}}">
    <view class="cml-checkbox-check  cml-base cml-view cml-557bcfae" style="{{checkStyle}}">
      <view class="{{ checkClass }}  cml-base cml-view cml-557bcfae">
        <image class="inner-icon  cml-base cml-image cml-557bcfae" src="{{ checkImg }}" />
      </view>
    </view>
    <text class="cml-checkbox-label  cml-base cml-text cml-557bcfae">{{label}}</text>
  </view>
</view>